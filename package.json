{
  "name": "module-express-and-layered-architecture-postgre",
  "version": "1.0.0",
  "description": "Express and Layered Architecture",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/server",
    "start:dev": "cross-env ENVIRONMENT=development tsc-watch --onSuccess \"node dist/server\"",
    "start:prod": "cross-env ENVIRONMENT=production tsc-watch --onSuccess \"node dist/server\"",
    "migration:create": "npx mikro-orm migration:create",
    "container:start": "podman-compose up -d",
    "migration:up": "npx mikro-orm migration:up",
    "seeder:run-product": "npx mikro-orm seeder:run --class=ProductSeeder",
    "shutdown": "podman-compose down && podman machine stop && brew services stop postgresql"
  },
  "keywords": [],
  "author": "heorhii okarskyi",
  "license": "ISC",
  "ts-node": {
    "esm": true,
    "transpileOnly": true
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config/orm.config.ts",
      "./dist/config/orm.config.js"
    ]
  },
  "dependencies": {
    "@mikro-orm/core": "^6.3.1",
    "@mikro-orm/migrations": "6.3.1",
    "@mikro-orm/postgresql": "^6.3.1",
    "@mikro-orm/reflection": "6.3.1",
    "@mikro-orm/seeder": "6.3.1",
    "bcrypt": "^5.1.1",
    "csv-writer": "^1.6.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastify": "^4.28.1",
    "joi": "^17.13.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "6.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.13",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "eslint": "^9.11.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0",
    "vitest": "^2.0.5"
  }
}
